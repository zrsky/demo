page {
  color: #333;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
  padding:0 20rpx;
}
.fts32 {
  font-size: 32rpx;
}
.pd24 {
  padding: 0 24rpx;
}
.pd28 {
  padding: 28rpx;
}
.mt20 {
  margin-top:20rpx; 
}
button::after{
  border:none;
}
@font-face {
  font-family: 'iconfont';  /* project id 729471 */
  src: url('//at.alicdn.com/t/font_729471_u3biu1bjxrq.eot');
  src: url('//at.alicdn.com/t/font_729471_u3biu1bjxrq.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_729471_u3biu1bjxrq.woff') format('woff'),
  url('//at.alicdn.com/t/font_729471_u3biu1bjxrq.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_729471_u3biu1bjxrq.svg#iconfont') format('svg');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-youjiantou:before { content: "\e612"; }

.icon-xiajiantou:before { content: "\e731"; }

.icon-guanbi:before { content: "\e624"; }

